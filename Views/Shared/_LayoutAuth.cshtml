<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - JobExchange</title>
    <link rel="stylesheet" href="~/css/app.min.99dbaee6ec838d8e.css">
    <link rel="stylesheet" href="~/css/job-exchang_login.css">
    <link rel="stylesheet" href="~/css/component_login.css">
    <style>
        #toast-container > div {
            opacity: unset;
        }

        [v-cloak] {
            display: none !important;
        }
    </style>
</head>
<body>
    <div class="wrap-auth-section">
        <div class="auth row">
            <div class="auth-form">
                @RenderBody()
            </div>
        </div>
    </div>
    <p class="auth-copy-right">© 2023-2023 Job Exchange ABCG07.</p>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
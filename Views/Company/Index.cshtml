@{
    ViewData["Title"] = "Company Page";
}
<div class="company">
<div class="box-search-company">
    <div class="container d-flex">
        <div class="box-search">
            <ul class="nav header">
                <li class="nav-item">
                    <a asp-controller="Company" asp-action="Index" class="nav-link  active ">Danh sách công ty</a>
                </li>
                <li class="nav-item">
                    <a href="top-cong-ty.html" class="nav-link ">Top công ty</a>
                </li>
            </ul>
            <div class="caption">
                <h1 class="title">Khám phá 100.000+ công ty nổi bật</h1>
                <p class="description">Tra cứu thông tin công ty và tìm kiếm nơi làm việc tốt nhất dành cho
                    bạn</p>
            </div>
            <form action="https://www.topcv.vn/cong-ty/tim-kiem" class="form-search border-hover">
                <i class="fa-light fa-magnifying-glass"></i>
                <input class="form-control" placeholder="Nhập tên công ty" id="keyword" name="keyword">
                <button class="btn btn-search btn-search-company btn-primary-hover">
                    Tìm kiếm
                </button>
            </form>
        </div>
        <div class="box-img">
            <img src="v4/image/brand-identity/company-billBoarde209.png?v=1.0.0" class="" alt=""
                style="width: 272px !important;">
        </div>
    </div>  
</div>
